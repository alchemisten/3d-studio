{
  "name": "@alchemisten/3d-studio-viewer-core",
  "version": "0.1.0",
  "description": "The core implementation for a 3d viewer based on threejs.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --port 9000 --progress --profile --static example/ --mode development",
    "start.prod": "webpack serve --port 9000 --progress --profile --static example/ --mode production",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript",
    "threejs",
    "3d",
    "viewer"
  ],
  "author": "Alchemisten AG",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.7.13",
    "@types/three": "^0.131.0",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1"
  },
  "dependencies": {
    "inversify": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "three": "^0.132.0"
  }
}
