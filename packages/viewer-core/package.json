{
  "name": "@alchemisten/3d-studio-viewer-core",
  "version": "0.1.0",
  "description": "The core implementation for a 3d viewer based on threejs.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --port 9000 --inline --progress --profile --content-base example/ --mode development",
    "start.prod": "webpack serve --port 9000 --inline --progress --profile --content-base example/ --mode production",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript",
    "threejs",
    "3d",
    "viewer"
  ],
  "author": "Alchemisten AG",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "7.0.0",
    "@types/three": "^0.128.0",
    "@types/uuid": "^8.3.0",
    "source-map-loader": "0.2.3",
    "ts-loader": "^9.1.2",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@alchemisten/gltf-extension": "^1.0.0",
    "inversify": "^5.1.1",
    "inversify-binding-decorators": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.1",
    "three": "^0.128.0",
    "uuid": "^8.3.2"
  }
}
